<h1>Groups#new</h1>
<p>Find me in app/views/groups/new.html.erb</p>

<%= simple_form_for @group do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.input :photo, as: :file %>

  <%= f.submit %>
<% end %>
